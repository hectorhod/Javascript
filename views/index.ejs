<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% docs.forEach(function(users){ %>
        <li>
            <!--<%= users.username %> | <%= users.email %>-->
            <a href="mailto:<%= users.mail %>"><%= users.username %></a>
        </li>
      <% }) %>
    </ul>
  </body>
</html>
